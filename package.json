{
  "name": "vue3-typescript-vite-thiago",
  "version": "0.0.3",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/ThiagoBrunoPereira/vuejs3-typescript-vite.git"
  },
  "author": "Thiago Bruno Pereira",
  "license": "MIT",
  "files": [
    "dist",
    "dist_types",
    "README.md"
  ],
  "main": "./dist/vue3-typescript-vite-thiago.mjs",
  "module": "./dist/vue3-typescript-vite-thiago.umd.js",
  "typings": "./dist_types/index.d.ts",
  "style": "./dist/style.css",
  "scripts": {
    "dev": "vitepress dev docs --port 3333",
    "start": "vitepress serve docs --port $PORT",
    "build": "vitepress build docs && cp -r ./docs/assets/* ./docs/.vitepress/dist/assets/",
    "build:package": "vite build",
    "build:types": "vue-tsc --declaration --emitDeclarationOnly && tsc-alias -p tsconfig.json",
    "build:production": "npm run build:types && npm run build:package",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build-only": "vite build",
    "typecheck": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "dependencies": {
    "@vueuse/core": "^9.3.0",
    "classnames": "^2.3.2",
    "pinia": "^2.0.32",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6",
    "@headlessui/vue": "^1.7.7",
    "@heroicons/vue": "2.0.5"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/jsdom": "^21.1.0",
    "@types/node": "^18.14.2",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitejs/plugin-vue-jsx": "^3.0.0",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/test-utils": "^2.3.0",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.34.0",
    "eslint-plugin-vue": "^9.9.0",
    "jsdom": "^21.1.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "postcss-import": "^15.1.0",
    "postcss-nested": "^6.0.0",
    "postcss-prefix-selector": "^1.16.0",
    "prettier": "^2.8.4",
    "sass": "^1.56.2",
    "sass-loader": "^13.2.0",
    "tailwindcss": "^3.3.1",
    "tsc-alias": "^1.8.2",
    "typescript": "~4.8.4",
    "vite": "^4.1.4",
    "vitepress": "^1.0.0-alpha.65",
    "vitest": "^0.29.1",
    "vue-tsc": "^1.2.0"
  }
}
